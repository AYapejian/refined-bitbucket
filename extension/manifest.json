{
  "manifest_version": 2,
  "name": "Refined Bitbucket",
  "description": "Improves Bitbucket user interface",
  "version": "1.3.0",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://bitbucket.org/*"
  ],
  "web_accessible_resources": [
    "lib/prism.js",
    "lib/require-cs.js",
    "lib/require.js",
    "var/window.js",
    "var/document.js",
    "jquery.js",
    "var/languages.js",
    "app/highlighter.js",
    "app/doubleClickWordSelection.js"
  ],
  "content_scripts": [{
    "run_at": "document_end",
    "matches": [
      "https://bitbucket.org/*"
    ],
    "js": [
      "lib/require.js",
      "lib/require-cs.js",
      "lib/prism.js",
      "app/main.js"
    ]
  }],

  "author": "andremw",
  "homepage_url": "https://github.com/andremw/refined-bitbucket"

}
